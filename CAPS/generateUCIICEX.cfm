				<CFELSE>
					<CFSET FINALSTRING = "#FINALSTRING##DELIMINATOR##DELIMINATOR##DELIMINATOR##DELIMINATOR#">	
				</CFIF>
				
				<CFIF GetClientDetails.CATEGORY NEQ 'I' AND REGISTRATION_NO NEQ "" AND REGISTRATION_AUTHORITY NEQ "" AND REGISTRATION_PLACE NEQ "" AND REGISTRATION_DATE1 NEQ "">
					<CFSET FINALSTRING	   = "#FINALSTRING##UCASE(REGISTRATION_AUTHORITY)##DELIMINATOR##UCASE(REGISTRATION_NO)##DELIMINATOR##UCASE(REGISTRATION_PLACE)##DELIMINATOR##REGISTRATION_DATE1##DELIMINATOR#">
				<CFELSE>
					<CFSET FINALSTRING	   = "#FINALSTRING##DELIMINATOR##DELIMINATOR##DELIMINATOR##DELIMINATOR#">
				</CFIF>
				
				<CFSET FINALSTRING	   = "#FINALSTRING##INTRODUCER_NAME##DELIMINATOR##INTRODUCER_ACC_CODE##DELIMINATOR##RELWITHINT##DELIMINATOR##DELIMINATOR#N#DELIMINATOR#">
				
				<CFIF  DP_ID1 NEQ "" AND DEPOSITORY1 NEQ "" AND CLIENT_DP_CODE1 NEQ "" >
					<CFSET FINALSTRING = "#FINALSTRING##DEPOSITORY1##DELIMINATOR##DP_ID1##DELIMINATOR##DP_NAME1##DELIMINATOR##CLIENT_DP_CODE1##CHR(10)#">	
				<CFELSE>
					<CFSET FINALSTRING = "#FINALSTRING##DELIMINATOR##DELIMINATOR##DELIMINATOR##CHR(10)#">
				</CFIF>
							
				<!--- <CFSET FINALSTRING = "#FINALSTRING##CHR(10)#">
				<CFSET FINALSTRINGLOG = "#FINALSTRINGLOG##CLIENT_ID# SuccesFully Generated#chr(10)#"> --->
				<CFIF ERRORCODE EQ 1>
					<CFSET FINALSTRINGLOG = "For #CLIENT_ID# : Insert Proper Pan NO.#chr(10)#">
				<cfelseif ERRORCODE EQ 2>
					<CFSET FINALSTRINGLOG = "For #CLIENT_ID# : Tele Phone is Missing / Length is Not Proper.#chr(10)#">
				<cfelseif ERRORCODE EQ 3>
					<CFSET FINALSTRINGLOG = "For #CLIENT_ID# : Mobile NO. is Missing/ Length is Not Proper.#chr(10)#">
				<cfelseif ERRORCODE EQ 4>
					<CFSET FINALSTRINGLOG = "For #CLIENT_ID# : Birth Date / Date of Incorporation is Missing.#chr(10)#">	
				<cfelse>
					<CFSET FINALSTRINGLOG = "For #CLIENT_ID# : Generated SucessFully.#chr(10)#">		

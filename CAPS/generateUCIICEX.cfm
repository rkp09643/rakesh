				<CFELSE>
					<CFSET BANKTYPE 			= 	10>
				</CFIF>
				
				<CFSET CLIENTBANKCODE 			= 	"#LEFT(TRIM(GETBANKDETAILS.Bank_AcNo),25)#">
				
				<CFSET DP_ID1					=	"#LEFT(TRIM(GetclientDpDetails.DP_ID),25)#">
				<CFSET DEPOSITORY1				=	"#LEFT(TRIM(GetclientDpDetails.DEPOSITORY),4)#">
				<CFSET CLIENT_DP_CODE1 			= 	"#LEFT(TRIM(GetclientDpDetails.CLIENT_DP_CODE),16)#">
				<CFSET DP_NAME1 				= 	"#TRIM(GetclientDpDetails.DP_NAME)#">
				
				<CFSET OTHERACCOUNT				=	"">
				<CFSET SETTLEMENTMODE			=	"">
				<CFSET CLIENTOTH				=	"">
				<CFSET FLAG						=	"E">
				<CFSET FINALSTRING = "#FINALSTRING#">
				<CFIF LEN(MAPIN) NEQ 9 >
					<CFSET MAPIN="">
				</CFIF>
				
				<!--- <cfquery name="GETRESSTATECODE" datasource="#CLIENT.DATABASE#">
					SELECT *
					FROM STATE_MASTER
					WHERE
						STATE_CODE = '#R_STATE#'
				</cfquery> --->
				<CFSET RESSTATECODE = "91">


				<!--- #DELIMINATOR# --->
				<CFIF TRIM(RESI_TEL_NO1) EQ "">
					<cfset ERRORCODE = 2 >	
				</CFIF>
				
				<CFIF TRIM(MOBILE_NO1) EQ "">
					<cfset ERRORCODE = 3 >	
				</CFIF>
				
				<CFIF TRIM(BIRTHDATE) EQ "">
					<cfset ERRORCODE = 4 >	
				</CFIF>
				
				
				<CFSET FINALSTRING = "#FINALSTRING#1#DELIMINATOR##UCASE(CLIENTID)##DELIMINATOR##UCASE(PAN_NO1)##DELIMINATOR##UCASE(CATEGORY1)##DELIMINATOR##GENDER1##DELIMINATOR##DELIMINATOR##UCASE(FIRSTNAME)##DELIMINATOR##UCASE(LASTNAME)##DELIMINATOR##DELIMINATOR##BIRTHDATE##DELIMINATOR#IN#DELIMINATOR##DELIMINATOR##AGREEMENT_DATE1##DELIMINATOR#C#DELIMINATOR##DELIMINATOR##DELIMINATOR#Default#DELIMINATOR##LEFT(TRIM(REPLACE(UCASE(Add1),',',' ','ALL')),30)##DELIMINATOR##LEFT(TRIM(REPLACE(UCASE(Add2),',',' ','ALL')),30)##DELIMINATOR##LEFT(TRIM(REPLACE(UCASE(Add3),',',' ','ALL')),30)##DELIMINATOR##UCASE(R_CITY)##DELIMINATOR##UCASE(R_COUNTRY)##DELIMINATOR##UCASE(R_STATE)##DELIMINATOR##UCASE(PIN_CODE)##DELIMINATOR##RESSTATECODE##DELIMINATOR##Replace(REPLACE(RESI_TEL_NO1,',',' ','ALL'),'-','','All')##DELIMINATOR##UCASE(MOBILE_NO1)##DELIMINATOR##Replace(REPLACE(RESI_FAX_NO1,',',' ','ALL'),'-','','All')##DELIMINATOR##CLIENT_ID_MAIL##DELIMINATOR#">
				
				
				<!--- <CFIF Bank_Name NEQ "" AND Bank_Add NEQ "" AND CLIENTBANKCODE NEQ "" >
					<CFSET FINALSTRING = "#FINALSTRING##REPLACE(UCASE(Bank_name),',','','ALL')#,#REPLACE(UCASE(Bank_Add),',','','ALL')#,#UCASE(CLIENTBANKCODE)#,#BANKTYPE#,">	
				<CFELSE> --->
					<CFSET FINALSTRING = "#FINALSTRING##DELIMINATOR##DELIMINATOR##DELIMINATOR#INR#DELIMINATOR#">
<!--- 				</CFIF> --->
				
				<!--- <CFSET FINALSTRING = "#FINALSTRING#,,,,,,,,"> --->
				
				<!--- <CFIF DEPOSITORY1 EQ "NSDL">
					<CFIF  DP_ID1 NEQ "" AND DEPOSITORY1 NEQ "" AND CLIENT_DP_CODE1 NEQ "" >
						<CFSET FINALSTRING = "#FINALSTRING##UCASE(DEPOSITORY1)#,#DP_ID1#,#UCASE(DP_NAME1)#,#CLIENT_DP_CODE1#,,,,,,">	
					<CFELSE>
						<CFSET FINALSTRING = "#FINALSTRING#,,,,,,,,,">
					</CFIF>
				<cfelse>
					<CFIF  DP_ID1 NEQ "" AND DEPOSITORY1 NEQ "" AND CLIENT_DP_CODE1 NEQ "" >
						<CFSET FINALSTRING = "#FINALSTRING#,,,,,#DEPOSITORY1#,#DP_ID1#,#UCASE(DP_NAME1)#,#CLIENT_DP_CODE1#,">	
					<CFELSE>
						<CFSET FINALSTRING = "#FINALSTRING#,,,,,,,,,">
					</CFIF>	
				</CFIF> --->
				
				<CFIF DRIVING_LICENSE1 NEQ "" AND DRIVING_LICENSE_ISSUED_PLACE1 NEQ "" AND DRIVING_LICENSE_ISSUED_DATE NEQ "" AND dr_lic_exp_date1 NEQ "">
					<CFSET FINALSTRING = "#FINALSTRING##UCASE(DRIVING_LICENSE1)##DELIMINATOR##REPLACE(UCASE(DRIVING_LICENSE_ISSUED_PLACE1),',',' ','ALL')##DELIMINATOR##DRIVING_LICENSE_ISSUED_DATE##DELIMINATOR##dr_lic_exp_date1##DELIMINATOR#">
				<CFELSE>
					<CFSET FINALSTRING = "#FINALSTRING##DELIMINATOR##DELIMINATOR##DELIMINATOR##DELIMINATOR#">	
